<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style/report.css">
    <title>Report</title>
</head>
<body>

    <!-- navbar / sidebar -->
    <div class="nav-btn" data-status="close"><img src="img/menu-blue.png" alt=""></div>
    <div class="nav-open display-none"></div>
    <div class="navbar">
        <div class="card" data-href="/home"><span class="img"><img src="img/homepage.png" alt=""></span><span>Beranda</span></div>
        <div class="card" data-href="/product"><span class="img"><img src="img/box.png" alt=""></span><span>Produk</span></div>
        <div class="card" data-href="/selling"><span class="img"><img src="img/cart1.png" alt=""></span><span>Penjualan</span></div>
        <div class="card" data-href="/purchase"><span class="img"><img src="img/cart.png" alt=""></span><span>Pembelian</span></div>
        <div class="card" data-href="/expenses"><span class="img"><img src="img/expenses.png" alt=""></span><span>Pengeluaran</span></div>
        <div class="card" data-href="/inclusion"><span class="img"><img src="img/rupiah.png" alt=""></span><span>Pemasukan Lain</span></div>
        <div class="card actived" data-href="/report"><span class="img"><img src="img/graph.png" alt=""></span><span>Laporan</span></div>
        <div class="card" data-href="/setting"><span class="img"><img src="img/settings.png" alt=""></span><span>Pengaturan</span></div>
    </div>

    <div class="container">
        <div class="first-content content">
            <div class="title"><h2>Laporan Keuangan</h2></div>
            <div class="box">
                <div class="box-list" data-href="/report/finance/general">
                    <img src="img/draft.png" alt="">
                    <p>Umum / Laba Rugi</p>
                </div>
                <div class="box-list" data-href="/report/finance/general">
                    <img src="img/draft.png" alt="">
                    <p>Perubahan Ekuitas</p>
                </div>
                <div class="box-list" data-href="/report/finance/general">
                    <img src="img/draft.png" alt="">
                    <p>Posisi Keuangan</p>
                </div>
            </div>
        </div>
        <div class="second-content content">
            <div class="title"><h2>Laporan Barang / Stock</h2></div>
            <div class="box">
                <div class="box-list" data-href="/report/product/general">
                    <img src="img/draft.png" alt="">
                    <p>Umum</p>
                </div>
            </div>
        </div>
        <div class="third-content content">
            <div class="title"><h2>Laporan Penjualan</h2></div>
            <div class="box">
                <div class="box-list" data-href="/report/selling/general">
                    <img src="img/draft.png" alt="">
                    <p>Umum</p>
                </div>
            </div>
        </div>
        <div class="fourth-content content">
            <div class="title"><h2>Laporan Pembelian</h2></div>
            <div class="box">
                <div class="box-list" data-href="/report/purchase/general">
                    <img src="img/draft.png" alt="">
                    <p>Umum</p>
                </div>
            </div>
        </div>
        <div class="fifth-content content">
            <div class="title"><h2>Laporan Lain-lain</h2></div>
            <div class="box">
                <div class="box-list" data-href="/report/others/general">
                    <img src="img/draft.png" alt="">
                    <p>Umum</p>
                </div>
            </div>
        </div>
    </div>
    
</body>
<script src="script/report.js"></script>
</html>